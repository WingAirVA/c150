    <animation>
        <type>rotate</type>
        <object-name>VoxSwitch</object-name>
        <property alias="../../params/nav-audio"/>
        <factor>-40</factor>
        <offset-deg>20</offset-deg>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>1.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0659015</y-m>
            <z-m>-0.0218897</z-m>
        </center>
    </animation>


    <animation>
        <type>pick</type>
        <object-name>VoxSwitch</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property alias="/params/nav-audio"/>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>nav-vox-switch</tooltip-id>
                <label>Nav Vox / IDT: %s</label>
                <property alias="/params/nav-audio"/>
                <mapping>nasal</mapping>
                <script>
                    var m = arg[0];
                    if (m == 0) return 'IDT';
                    return 'VOX';
                </script>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FiveSwitch</object-name>
        <property alias="../../params/nav-five-switch"/>
        <factor>-40</factor>
        <offset-deg>20</offset-deg>
        <axis>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0166174</z-m>
        </center>
    </animation>


    <animation>
        <type>pick</type>
        <object-name>FiveSwitch</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property alias="/params/nav-five-switch"/>
            </binding>
        </action>
    </animation>

   <animation>
    <type>knob</type>
    <object-name>CommSquelch</object-name>
    <property alias="../../params/comm-squelch"/>
    <factor>-120</factor>
    <offset-deg>30</offset-deg>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.013</x-m>
      <y-m>-0.0358325</y-m>
      <z-m>-0.0218737</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../params/comm-squelch"/>
        <factor>0.1</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>comm-squelch</tooltip-id>
        <label>Squelch: %3d%%</label>
        <property alias="../../../../params/comm-squelch"/>
        <mapping>percent</mapping>
      </binding>
    </hovered>
  </animation>


  <animation>
    <type>knob</type>
    <object-name>NavVolume</object-name>
    <property alias="../../params/nav-volume"/>
    <factor>-120</factor>
    <offset-deg>30</offset-deg>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.013</x-m>
      <y-m>0.0362975</y-m>
      <z-m>-0.0218737</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../params/nav-volume"/>
        <factor>0.1</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>nav-volume</tooltip-id>
        <label>Volume: %3d%%</label>
        <property alias="../../../../params/nav-volume"/>
        <mapping>percent</mapping>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>CommVolume</object-name>
    <property alias="../../params/comm-volume"/>
    <factor>-120</factor>
    <offset-deg>30</offset-deg>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.013</x-m>
      <y-m>-0.0655902</y-m>
      <z-m>-0.0218737</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/comm-volume"/>
        <factor>0.1</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>

      <binding>
        <command>property-assign</command>
        <property alias="/params/power"/>
        <value>1</value>
        <condition>
          <greater-than>
            <property alias="/params/comm-volume"/>
            <value>0.05</value>
          </greater-than>
        </condition>
      </binding>
      <binding>
        <command>property-assign</command>
        <property alias="/params/power"/>
        <value>0</value>
        <condition>
          <less-than>
            <property alias="/params/comm-volume"/>
            <value>0.05</value>
          </less-than>
        </condition>
      </binding>
    </action>
  <decrease>
    <binding>
        <command>nasal</command>
        <script>c150.PlaySound("click-avionics")</script>
        <condition>
          <less-than>
            <property alias="/params/comm-volume"/>
            <value>0.05</value>
          </less-than>
        </condition>
    </binding>
  </decrease>

  <increase>
    <binding>
        <command>nasal</command>
        <script>c150.PlaySound("click-avionics")</script>
        <condition>
          <less-than>
            <property alias="/params/comm-volume"/>
            <value>0.15</value>
          </less-than>
        </condition>
    </binding>
  </increase>



    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>comm-volume</tooltip-id>
        <label>Volume: %3d%%</label>
        <property alias="/params/comm-volume"/>
        <mapping>percent</mapping>
      </binding>
    </hovered>
  </animation>


  <animation>
    <type>knob</type>
    <object-name>CommKHz</object-name>
    <property alias="../../params/comm-frq-selected"/>
    <expression>
        <product>
            <difference>
                <property alias="/params/comm-frq-selected"/>
                <floor>
                    <property alias="/params/comm-frq-selected"/>
                </floor>
            </difference>
            <value>-360.0</value>
        </product>
    </expression>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.021</x-m>
      <y-m>-0.022141</y-m>
      <z-m>-0.00551775</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/comm-frq-selected"/>
        <factor>0.025</factor>
        <min>0.0</min>
        <max>1.0</max>
        <resolution>0.025</resolution>
        <wrap>true</wrap>
        <mask>decimal</mask>
      </binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>CommMHz</object-name>
    <property alias="../../params/comm-frq-selected"/>
    <expression>
        <product>
            <value>-15</value>
            <floor>
                <property alias="/params/comm-frq-selected"/>
            </floor>
        </product>
    </expression>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.021</x-m>
      <y-m>-0.0519498</y-m>
      <z-m>-0.00551775</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/comm-frq-selected"/>
        <factor>1</factor>
        <min>118.0</min>
        <max>137.0</max>
        <wrap>false</wrap>
        <mask>integer</mask>
      </binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>NavKHz</object-name>
    <property alias="../../params/nav-frq-selected"/>
    <expression>
        <product>
            <difference>
                <property alias="/params/nav-frq-selected"/>
                <floor>
                    <property alias="/params/nav-frq-selected"/>
                </floor>
            </difference>
            <value>-360.0</value>
        </product>
    </expression>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.021</x-m>
      <y-m>0.0526992</y-m>
      <z-m>-0.00551775</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/nav-frq-selected"/>
        <factor>0.05</factor>
        <min>0.0</min>
        <max>1.0</max>
        <resolution>0.050</resolution>
        <wrap>true</wrap>
        <mask>decimal</mask>
      </binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>NavMHz</object-name>
    <property alias="../../params/nav-frq-selected"/>
    <expression>
        <product>
            <value>-15</value>
            <floor>
                <property alias="/params/nav-frq-selected"/>
            </floor>
        </product>
    </expression>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.021</x-m>
      <y-m>0.0228706</y-m>
      <z-m>-0.00551775</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/nav-frq-selected"/>
        <factor>1</factor>
        <min>108.0</min>
        <max>118.0</max>
        <wrap>false</wrap>
        <mask>integer</mask>
      </binding>
    </action>
  </animation>

 

